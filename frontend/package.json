{
  "name": "connect-frontend",
  "description": "Connect Frontend and WebRTC to the backend",
  "author": {
    "name": "Arvind",
    "email": "lordarvindthegamer@gmail.com",
    "url": "https://arvind11.tk/",
    "username": "Arvind-4"
  },
  "scripts": {
    "dev:react": "vite --config viteConfig/react.config.js",
    "dev:js": "vite --config viteConfig/js.config.js",
    "watch:css": "rm -rf ../web/static-dev/css/style.min.css && tailwind build -i ./tailwind/tailwind.css -o ../web/static-dev/css/style.min.css --watch",
    "build:css": "NODE_ENV=production postcss tailwind/tailwind.css -o ../web/static-dev/css/style.min.css",
    "build:react": "vite build --config viteConfig/react.config.js && cd ../web/static-dev/js && rm -rf index.html && mv assets/*.js ./ && mv assets/*.css ../css/index.min.css && rm -rf assets && mv index.*.js ./index.min.js",
    "build:js": "rm -rf ../web/static-dev/js/script.min.js && vite build --config viteConfig/js.config.js && cd ../web/static-dev/js/ && rm -rf index.html && mv assets/*.js script.min.js && rm -rf assets",
    "production": "yarn build:production:css && yarn build:production:js && yarn build:production:react"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.16",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.0",
    "tailwindcss": "^3.0.18",
    "vite": "^2.7.2"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tailwindcss/forms": "^0.4.0",
    "aos": "^3.0.0-beta.6",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flatpickr": "^3.10.7",
    "react-router-dom": "^6.2.1",
    "react-transition-group": "^4.4.1",
    "sticky-js": "^1.3.0"
  },
  "license": "MIT",
  "version": "1.0.0",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Arvind-4/Connect"
  },
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1",
    "yarn": "1.22.17"
  },
  "keywords": [
    "connect",
    "frontend",
    "webrtc",
    "website",
    "website-frontend",
    "website-webrtc",
    "website-website",
    "website-website-frontend",
    "website-website-webrtc"
  ]
}
